public class Grades3 {
    void grades(){
        int grade = 90;

        if (grade >= 70)
            System.out.println("A");
        else if (grade >= 60)
            System.out.println("B");
        else if (grade >= 50)
            System.out.println("C");
        else
            System.out.println("Fail");

        }
    public  static void main (String[] args) {
        (new Grades3()).grades();
    }
}
